{
  "mcpServers": {
    "abuseipdb-docker": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "--name", "abuseipdb-mcp",
        "-e", "ABUSEIPDB_API_KEY",
        "abuseipdb-mcp"
      ],
      "env": {
        "ABUSEIPDB_API_KEY": "your_api_key_here"
      }
    }
  }
} 